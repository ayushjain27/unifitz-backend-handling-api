{
  "name": "service-plug",
  "version": "1.0.0",
  "description": "ServicePlug",
  "author": "JSH",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --config .prettierrc --write **/*.ts",
    "tsc": "tsc",
    "watch-tsc": "tsc -w",
    "deploy": "node dist/src/server.js",
    "watch-deploy": "nodemon dist/src/server.js",
